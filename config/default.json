{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "csw": {
    "raster": {
      "url": "http://localhost:8000/pycsw/version=2.0.0&service=csw"
    },
    "3d": {
      "url": "http://localhost:8000/pycsw/version=2.0.0&service=csw"
    }
  },
  "catalogServices": {
    "raster": {
      "url": "http://localhost:8000/catalog-service-raster"
    },
    "3d": {
      "url": "http://localhost:8000/catalog-service-3d"
    }
  },
  "ingestionServices": {
    "raster": {
      "url": "http://localhost:8000/ingestion-service-raster"
    },
    "3d": {
      "url": "http://localhost:8000/ingestion-service-3d"
    }
  },
  "jobServices": {
    "raster": {
      "url": "http://job-manager-dev-job-manager:80"
    },
    "3d": {
      "url": "http://localhost:8080"
    }
  }
}
