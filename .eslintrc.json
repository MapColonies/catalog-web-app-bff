{
  "ignorePatterns": ["**/*.js", "dist", "src/AUTOGENERATED"],
  "extends": ["@map-colonies/eslint-config/jest", "@map-colonies/eslint-config/ts-base"],
  "parserOptions": {
    "project": "./tsconfig.lint.json"
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "type|of"
      }
    ]
  }
}
